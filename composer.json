{
    "name": "symplify/modular-routing",
    "description": "Load your routes with a simple service.",
    "keywords": ["Symfony", "routing", "provider", "modular"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/config": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/http-kernel": "^2.8|^3.0",
        "symfony/routing": "^2.8|^3.0",
        "symfony-cmf/routing-bundle": "^1.3"
    },
    "require-dev": {
        "nette/utils": "^2.3",
        "phpunit/phpunit": "^5.1",
        "symfony/expression-language": "^2.8|^3.0",
        "symfony/form": "^2.8|^3.0",
        "symplify/coding-standard": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularRouting\\Tests\\": "tests"
        }
    }
}
